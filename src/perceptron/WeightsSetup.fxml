<?xml version="1.0" encoding="UTF-8"?>


<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<AnchorPane prefHeight="400.0" prefWidth="601.0" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="perceptron.WeightsSetupController">
   <children>
      <VBox alignment="CENTER" prefHeight="400.0" prefWidth="601.0" spacing="10.0">
         <children>
            <Label alignment="TOP_CENTER" prefHeight="38.0" prefWidth="243.0" text="Weights Setup">
               <font>
                  <Font size="35.0" />
               </font>
               <padding>
                  <Insets bottom="10.0" top="10.0" />
               </padding>
            </Label>
            <ScrollPane prefHeight="257.0" prefWidth="601.0">
               <content>
                  <HBox fx:id="layerHBox" prefHeight="100.0" prefWidth="200.0" spacing="10.0" />
               </content>
            </ScrollPane>
            <Button fx:id="saveWeightsBtn" mnemonicParsing="false" onAction="#saveWeights" text="SaveWeights" />
         </children>
      </VBox>
   </children>
</AnchorPane>
